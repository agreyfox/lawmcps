#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/openai@4.96.0_encoding@0.1.13_ws@8.18.2_bufferutil@4.0.8_utf-8-validate@6.0.4__zod@3.25.32/node_modules/openai/bin/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/openai@4.96.0_encoding@0.1.13_ws@8.18.2_bufferutil@4.0.8_utf-8-validate@6.0.4__zod@3.25.32/node_modules/openai/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/openai@4.96.0_encoding@0.1.13_ws@8.18.2_bufferutil@4.0.8_utf-8-validate@6.0.4__zod@3.25.32/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/openai@4.96.0_encoding@0.1.13_ws@8.18.2_bufferutil@4.0.8_utf-8-validate@6.0.4__zod@3.25.32/node_modules/openai/bin/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/openai@4.96.0_encoding@0.1.13_ws@8.18.2_bufferutil@4.0.8_utf-8-validate@6.0.4__zod@3.25.32/node_modules/openai/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/openai@4.96.0_encoding@0.1.13_ws@8.18.2_bufferutil@4.0.8_utf-8-validate@6.0.4__zod@3.25.32/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
