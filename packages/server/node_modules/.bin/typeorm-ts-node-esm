#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.2_mysql2@3.11.4_pg@8.11.3_redis@4.7.0_sqlite3@5.1.7_ts-node@_2ace279e1535121d5790119d4764726b/node_modules/typeorm/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.2_mysql2@3.11.4_pg@8.11.3_redis@4.7.0_sqlite3@5.1.7_ts-node@_2ace279e1535121d5790119d4764726b/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.2_mysql2@3.11.4_pg@8.11.3_redis@4.7.0_sqlite3@5.1.7_ts-node@_2ace279e1535121d5790119d4764726b/node_modules/typeorm/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.2_mysql2@3.11.4_pg@8.11.3_redis@4.7.0_sqlite3@5.1.7_ts-node@_2ace279e1535121d5790119d4764726b/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
