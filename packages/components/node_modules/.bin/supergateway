#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/supergateway@3.0.1_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/supergateway/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/supergateway@3.0.1_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/supergateway/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/supergateway@3.0.1_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/supergateway@3.0.1_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/supergateway/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/supergateway@3.0.1_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/supergateway/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/supergateway@3.0.1_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../supergateway/dist/index.js" "$@"
else
  exec node  "$basedir/../supergateway/dist/index.js" "$@"
fi
