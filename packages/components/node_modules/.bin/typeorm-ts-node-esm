#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_socks@2.8.1__mysql2@3.11.4_pg@8.11.3_redis@4_dd5cf342d3e8c86050817c68db5bc212/node_modules/typeorm/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_socks@2.8.1__mysql2@3.11.4_pg@8.11.3_redis@4_dd5cf342d3e8c86050817c68db5bc212/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_socks@2.8.1__mysql2@3.11.4_pg@8.11.3_redis@4_dd5cf342d3e8c86050817c68db5bc212/node_modules/typeorm/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_socks@2.8.1__mysql2@3.11.4_pg@8.11.3_redis@4_dd5cf342d3e8c86050817c68db5bc212/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
