#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules/composio-core/dist/cli/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules/composio-core/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules/composio-core/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules/composio-core/dist/cli/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules/composio-core/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules/composio-core/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/composio-core@0.4.7/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../composio-core/dist/cli/index" "$@"
else
  exec node  "$basedir/../composio-core/dist/cli/index" "$@"
fi
