#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules/@flowiseai/nodevm/bin/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules/@flowiseai/nodevm/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules/@flowiseai/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules/@flowiseai/nodevm/bin/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules/@flowiseai/nodevm/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules/@flowiseai/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@flowiseai+nodevm@3.9.25/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@flowiseai/nodevm/bin/vm2" "$@"
else
  exec node  "$basedir/../@flowiseai/nodevm/bin/vm2" "$@"
fi
