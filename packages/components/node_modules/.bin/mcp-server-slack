#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules/@modelcontextprotocol/server-slack/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules/@modelcontextprotocol/server-slack/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules/@modelcontextprotocol/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules/@modelcontextprotocol/server-slack/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules/@modelcontextprotocol/server-slack/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules/@modelcontextprotocol/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-slack@2025.1.17/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@modelcontextprotocol/server-slack/dist/index.js" "$@"
else
  exec node  "$basedir/../@modelcontextprotocol/server-slack/dist/index.js" "$@"
fi
