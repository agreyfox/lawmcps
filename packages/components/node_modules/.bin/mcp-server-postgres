#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules/@modelcontextprotocol/server-postgres/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules/@modelcontextprotocol/server-postgres/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules/@modelcontextprotocol/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules/@modelcontextprotocol/server-postgres/dist/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules/@modelcontextprotocol/server-postgres/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules/@modelcontextprotocol/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/@modelcontextprotocol+server-postgres@0.6.2/node_modules:/home/lq/projects/liw/Flowise3.0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@modelcontextprotocol/server-postgres/dist/index.js" "$@"
else
  exec node  "$basedir/../@modelcontextprotocol/server-postgres/dist/index.js" "$@"
fi
